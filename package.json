{
  "name": "iban",
  "version": "0.0.14",
  "description": "A javascript library to validate, format and convert IBAN (International Bank Account Number) and BBAN (Basic Bank Account Number)",
  "main": "dist/iban.cjs.js",
  "module": "dist/iban.esm.js",
  "browser": "dist/iban.umd.js",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "@wessberg/rollup-plugin-ts": "1.3.8",
    "chai": "4.2.0",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.37.1",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "17.4.3",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rimraf dist/ && rollup -c",
    "postbuild": "cp dist/iban.esm.js demo/web-app/js && cp dist/iban.esm.d.ts demo/web-app/js && cp dist/iban.cjs.js demo/node/js && cp dist/iban.cjs.d.ts demo/node/js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": "https://github.com/arhs/iban.js",
